dnl Process this file with autoconf to produce a configure script.
AC_INIT(config/config.scoreboard)
dnl Replace the existing config.data with a message in case we abort.
echo "# configure did not complete successfully.  Resolve problems and re-run." >config/config.data
echo "# or obtain a known good config.data for your O/S" >>config/config.data

AC_PREFIX_DEFAULT(/usr/local/news)

dnl Parse the arguments
AC_ARG_ENABLE(pgp-verify,[  --enable-pgp-verify     use pgp control verification], DO_PGP=DO, DO_PGP=DONT)
AC_SUBST(DO_PGP)
dnl
AC_ARG_WITH(tcl,[  --with-tcl              TCL script support], DO_TCL=DO
TCL_LIB="-ltcl -lm"
TCL_INC="-I/usr/local/include", DO_TCL=DONT
TCL_LIB=""
TCL_INC="")
AC_SUBST(DO_TCL)
AC_SUBST(TCL_LIB)
AC_SUBST(TCL_INC)
dnl
AC_ARG_WITH(perl,[  --with-perl             Perl script support], DO_PERL=DO, DO_PERL=DONT)
AC_SUBST(DO_PERL)
dnl
AC_ARG_WITH(spool-dir,[  --with-spool-dir=PATH   News storage (default /var/news/spool)], SPOOLDIR=$with_spool_dir, SPOOLDIR=/var/news/spool)
AC_SUBST(SPOOLDIR)
dnl
AC_ARG_WITH(log-dir,[  --with-log-dir=PATH     Logging directory (default /var/log/news)], LOGDIR=$with_log_dir, LOGDIR=/var/log/news)
AC_SUBST(LOGDIR)
dnl
AC_ARG_WITH(news-user,[  --with-news-user=UID    News user id (default news)], NEWSUSER=$with_news_user, NEWSUSER=news)
AC_SUBST(NEWSUSER)
dnl
AC_ARG_WITH(news-group,[  --with-news-group=GID   News group id (default news)], NEWSGRP=$with_news_group, NEWSGRP=news)
AC_SUBST(NEWSGRP)
dnl
AC_ARG_WITH(tmp-path,[  --with-tmp-path=PATH    Temporary files directory], TMPPATH=$with_tmp_path, if test -d /var/tmp; then
	TMPPATH=/var/tmp
else
	if test -d /usr/tmp; then
		TMPPATH=/usr/tmp
	else
		TMPPATH=/tmp
	fi
fi) dnl HACK
AC_SUBST(TMPPATH)

dnl Fix the paths
test "x$prefix" = xNONE && prefix=$ac_default_prefix
test "x$exec_prefix" = xNONE && exec_prefix=$ac_default_prefix

AC_CACHE_CHECK(for MANDIR,MANDIR, MANDIR=`eval echo $mandir`)
AC_SUBST(MANDIR)

dnl Checks for programs.
AC_PROG_CC
AC_PROG_LN_S
AC_PROG_MAKE_SET
AC_PROG_YACC
AC_PROG_LEX
AC_PROG_RANLIB

dnl System specific
AC_AIX
AC_ISC_POSIX


dnl Checks for pathnames
ac_save_path="$PATH"

dnl 11/21/97 Path must be set before running.
dnl PATH="$PATH:/usr/local/sbin:/usr/local/bin:/usr/local/lib:/usr/ucb:/usr/ucb/bin:/lib:/usr/lib:/sbin:/usr/sbin"
dnl if the path mucking above must be done, we have to redo the echo test...
if (echo "testing\c"; echo 1,2,3) | grep c >/dev/null; then
  # Stardent Vistra SVR4 grep lacks -e, says ghazi@caip.rutgers.edu.
  if (echo -n testing; echo 1,2,3) | sed s/-n/xn/ | grep xn >/dev/null; then
    ac_n= ac_c='
' ac_t='	'
  else
    ac_n=-n ac_c= ac_t=
  fi
else
  ac_n= ac_c='\c' ac_t=
fi


AC_PATH_PROG(PATH_CTAGS,ctags,echo)

AC_CACHE_CHECK(for path to ctags,ac_cv_subst_ctags,
ac_cv_subst_ctags=$PATH_CTAGS
if test $ac_cv_subst_ctags = echo
then
	ac_cv_subst_ctags="$ac_cv_subst_ctags"
else
	ac_cv_subst_ctags="$ac_cv_subst_ctags -t -w"
fi)
AC_SUBST(ac_cv_subst_ctags)

dnl ***************************************************
dnl Run AC_PATH_PROG when it is an error to not find something.
AC_DEFUN(AC_ENSURE_PATH_PROG, [
AC_PATH_PROG($1,$2)
if test "x${$1}" = "x"
then
AC_MSG_ERROR($2 is a required program not found in path)
fi
])

AC_ENSURE_PATH_PROG(_PATH_AWK,awk)
AC_ENSURE_PATH_PROG(_PATH_PERL,perl)
AC_ENSURE_PATH_PROG(_PATH_SED,sed)
AC_ENSURE_PATH_PROG(_PATH_DF,df)
AC_ENSURE_PATH_PROG(_PATH_EGREP,egrep)
AC_ENSURE_PATH_PROG(_PATH_SORT,sort)
AC_ENSURE_PATH_PROG(_PATH_SH,sh)

dnl Look for PGP 5.0's pgpv, or pgp if pgpv is not found
AC_PATH_PROG(_PAGH_PGP,pgpv)
if test "x$_PATH_PGP" = x
then
AC_PATH_PROG(_PATH_PGP,pgp,no-pgp-found-during-configure)
fi

AC_PATH_PROG(ac_cv_subst_path_compress,compress,no-compress-found-during-configure)
AC_PATH_PROG(ac_cv_subst_path_gzip,gzip,no-gzip-found-during-configure)
dnl Automatic check for Mail is frequently wrong.  Use innmail to have
dnl a single interface.
dnl AC_ENSURE_PATH_PROG(ac_cv_subst_path_mail,Mail)
