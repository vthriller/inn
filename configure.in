	perl_core_path=`$ac_cv_subst_path_perl -e 'use Config; print $Config{archlib}'`
	perl_core_libs=`$ac_cv_subst_path_perl -e 'use Config; print $Config{libs}'`
	PERL_LIB="-L${perl_core_path}/CORE ${perl_core_libs}"
	PERL_INC="-I${perl_core_path}/CORE"
	AC_MSG_RESULT($perl_core_path)
else
	PERL_LIB=""
	PERL_INC=""
fi
