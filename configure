        if (!data3)
                exit(1);
        if (read(fd, data3, pagesize) != pagesize)
                exit(1);
        for (i = 0; i < pagesize; ++i)
                if (*(data + i) != *(data3 + i))
                        rval = 1;
        close(fd);
        unlink("conftestmmapm");
        exit(rval);
}

