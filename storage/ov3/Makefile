##  $Id$

include ../../Makefile.global

CFLAGS	= $(GCFLAGS) -I../../include -I..

SOURCES = ov3.c
OBJECTS = ov3.o
LOBJECTS = ov3.lo
ROBJECTS = ov3.$(EXTOBJ)

.SUFFIXES: .lo

all:			$(OBJECTS)

.c.o .c.lo:
	$(UPLIBCC) $(CFLAGS) -c $*.c -o $@

clobber clean:
	rm -f *.o *.lo obj/*.o
	rm -f profiled
	rm -f all install lint lint.all
	rm -rf .libs

tags ctags:	$(SOURCES)
	$(CTAGS) $(SOURCES) ../../include/*.h ../*.h

##  Dependencies.  Default list, below, is probably good enough.

depend:	Makefile $(SOURCES)
	$(UPMAKEDEPEND) $(CFLAGS) $(SOURCES)

# DO NOT DELETE THIS LINE -- make depend depends on it.
ov3.o: ov3.c ../../include/config.h ../../include/inn/defines.h \
 ../../include/clibrary.h ../../include/libinn.h \
 ../../include/macros.h ../../include/ov.h ../../include/storage.h \
 ../../include/paths.h ../ovinterface.h tradindexed.h
